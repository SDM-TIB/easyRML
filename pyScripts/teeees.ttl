@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix sd: <dfhdfhsfhs> .

<drug>
	rml:logicalSource [ rml:source "";
				rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/vocab/Disorder/{Gene CDS length}_{ID_sample}";
		rr:termType rr:IRI;
	];
	rr:predicateObjectMap [
		rr:predicate http://clarify2020.eu/vocab/existenceOf_bioRos1;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/vocab/DrugDrugInteraction";
			rr:termType rr:IRI;
		];
	].
<disorder>
	rml:logicalSource [ rml:source "";
				rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:constant "http://clarify2020.eu/vocab/Relation";
		rr:termType rr:IRI;
	];
	rr:predicateObjectMap [
		rr:predicate http://clarify2020.eu/vocab/hasDiagnosisCancerStageCategory;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/Disorder/{Accession Number}_{ID_tumour}";
			rr:termType rr:IRI;
		];
	];
	rr:predicateObjectMap [
		rr:predicate http://clarify2020.eu/vocab/target_isRelatedTo_gene;
		rr:objectMap [
			rr:parentTriplesMap <drug>;
			rr:termType rr:IRI;
		];
	].