@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<whatever>
	rml:logicalSource [ rml:source "/mnt/e/data/";
					rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:constant "http://clarify2020.eu/vocab/DrugEnzymeInteraction";
		rr:termType rr:IRI;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasMutation;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/vocab/Disorder";
			rr:termType rr:IRI;
		];
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasHPOAnnotation;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/vocab/DrugTargetInteraction";
			rr:termType rr:IRI;
		];
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_bioRos1;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/vocab/Disorder";
			rr:termType rr:IRI;
		];
	].